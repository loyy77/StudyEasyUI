                <!--     <param name="root">bookList</param>  -->
                <!--     <param name="defaultEncoding">utf-8</param> -->
                <param name="contentType">text/html</param>
            </result>
        </action>
        <action name="bookAJAX_add" class="loyy.action.BookAJAXAction" method="add">
            <result type="json">
                <!--     <param name="root">bookList</param>  -->
                <!--     <param name="defaultEncoding">utf-8</param> -->
                <param name="contentType">text/html</param>
            </result>
        </action>
        <action name="bookAJAX_list" class="loyy.action.BookAJAXAction"
                method="list">
            <result type="json">
                <param name="root">bookList</param>
                <!--     <param name="defaultEncoding">utf-8</param> -->
            </result>
        </action>
        <!-- Add actions here -->
    </package>
</struts>
